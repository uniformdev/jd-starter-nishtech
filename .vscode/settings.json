{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.configPath": ".prettierrc.js",
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "files.eol": "\n",
  "search.exclude": {
    "node_modules": true
  },
  "cSpell.words": [
    "acumin",
    "bigcommerce",
    "Callout",
    "cloudinary",
    "commitlint",
    "ctfassets",
    "Gtag",
    "manypkg",
    "nextjs",
    "Saionaro",
    "tailwindcss",
    "uniformdev",
    "unoptimized"
  ],
  "workbench.colorCustomizations": {
    "activityBar.background": "#461B4E",
    "titleBar.activeBackground": "#62266E",
    "titleBar.activeForeground": "#FEFCFE"
  }
}
